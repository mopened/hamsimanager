cb				=	MComboBox
wv				=	MWebView
sa				=	MScrollArea
tb				=	MToolButton
sld				=	MSlider
sci				=	QsciScintilla
tbox			=	MToolBox
tbar			=	MToolBar
gbox			=	MGroupBox
trv				=	MTreeView
lstv			=	MListView
act				=	MAction
te				=	MTextEdit
fnt				=	MFont
tabw			=	MTabWidget
cckb			=	MCheckBox
sp				=	MSpinBox
pmap			=	MPixmap
mbox			=	MMessageBox
actg			=	MActionGroup
d				=	MDialog
scra			=	MScrollArea
icn				=	QIcon
